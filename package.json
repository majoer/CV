    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.1.2",
    "babelify": "^7.2.0",
    "express": "^4.13.3",
    "history": "^1.13.1",
    "immstruct": "^2.0.0",
    "immutable": "^3.7.5",
    "json-loader": "^0.5.4",
    "omnipotent": "^1.2.1",
    "omniscient": "^4.1.1",
    "qwest": "^2.2.4",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-formdata": "^1.0.0",
    "react-router": "^1.0.1",
